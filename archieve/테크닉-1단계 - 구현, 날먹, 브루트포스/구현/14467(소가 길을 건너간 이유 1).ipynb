   "source": [
    "N = int(input())\n",
    "D = {}\n",
    "\n",
    "ans = 0\n",
    "for _ in range(N):\n",
    "  cow, pos = map(int, input().split())\n",
    "  if cow in D:\n",
    "    ans += D[cow] != pos\n",
    "    D[cow] = pos\n",
    "  else :\n",
    "    D[cow] = pos\n",
    "\n",
    "print(ans)"
   ]
  }
